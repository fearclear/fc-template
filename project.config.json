{
  "description": "项目配置文件",
  "packOptions": {
    "ignore": []
  },
  "miniprogramRoot": "dist/",
  "compileType": "miniprogram",
  "libVersion": "2.10.4",
  "projectname": "Your project name",
  "scripts": {
    "beforeCompile": "npm run compile",
    "beforePreview": "npm run compile",
    "beforeUpload": "npm run compile"
  },
  "setting": {
    "urlCheck": false,
    "es6": true,
    "enhance": true,
    "postcss": true,
    "preloadBackgroundData": false,
    "minified": true,
    "newFeature": false,
    "coverView": true,
